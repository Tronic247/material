{
    "name": "tronic247-material",
    "version": "3.0.1",
    "description": "Modern Material design framework",
    "main": "index.js",
    "scripts": {
        "build": "webpack && npm run build_css",
        "build_css": "node-sass src/scss/index.scss dist/material.css && npm run prefix_css",
        "prefix_css": "npx postcss dist/material.css --use autoprefixer --map false -d dist/ && css-minify -f dist/material.css -o dist"
    },
    "keywords": ["material-design"],
    "author": "tronic247",
    "license": "ISC",
    "devDependencies": {
        "@babel/core": "^7.15.8",
        "@babel/preset-env": "^7.15.8",
        "babel-loader": "^8.2.2",
        "webpack": "^5.58.2",
        "webpack-cli": "^4.9.0",
        "terser-webpack-plugin": "^5.3.0",
        "cssnano": "^5.0.14"
    },
    "dependencies": {
        "@popperjs/core": "^2.11.0",
        "focus-trap": "^6.7.1",
        "nouislider": "^15.5.0",
        "tabbable": "^5.2.1",
        "node-sass": "^7.0.1",
        "postcss": "^8.4.5",
        "postcss-cli": "^9.1.0",
        "autoprefixer": "^10.4.0",
        "css-minify":"^2.0.0"
    }
}
